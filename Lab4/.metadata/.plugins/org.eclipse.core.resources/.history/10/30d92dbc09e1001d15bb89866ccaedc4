
public class Acticity3 {

	public static void main(String[] args) {
		boolean result;
		int Newnumber = 2;
		result = isPrime(Newnumber);
		System.out.println(result);

	}

	public static boolean isPrime(int n) {
		for (int i = n; i < 10000; i++) {
			for(int j=1;j<=i/2;j++)
			if (i%j != 0) {
				return false;
			}
		}
		return true;

	}
}
